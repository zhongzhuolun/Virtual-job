page {
  background-color: rgba(255, 255, 255, 1);
}
.bank-wrap {
  background: white;
  padding-bottom: 5vh;
}
.bank-wrap .line {
  width: 750rpx;
  border: 1rpx solid rgba(187, 187, 187, 1);
}
.bank {
  margin-left: 34rpx;
  overflow: auto;
  padding-top: 22rpx;
  max-height: 60vh;
  min-height: 50vh;
}
.bank-title {
  margin-left: 34rpx;
  padding-top: 42rpx;
  font-size: 50rpx;
  color: rgba(16, 16, 16, 1);
  text-align: left;
  font-family: PingFangSC-regular;
  width: 80vw;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}

