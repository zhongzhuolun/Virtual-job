page {
  background: white;
}
.written, .interview{
  display: inline-block;
  width: 366rpx;
  opacity: 0.8;
  line-height: 104rpx;
  border-radius: 8rpx;
  text-align: center;
  border: 2rpx solid rgba(187, 187, 187, 1);
  background: #fff;
}
.written {
  left: 0px;
}
.wrap .active {
  width: 384rpx;
  opacity: 1;
}

.interview {
  left: 384rpx;
  width: 366rpx;
  opacity: 0.8;
}
