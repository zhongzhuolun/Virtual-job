.bank-wrap {
  background: white;
  padding-bottom: 8vh;
}
.bank {
  margin-left: 34rpx;
  padding-top: 45rpx;
  overflow: auto;
  min-height: 50vh;
  max-height: 60vh;
}
.bank-title {
  margin-left: 34rpx;
  padding-top: 42rpx;
  color: rgba(16, 16, 16, 1);
  font-size: 50rpx;
  text-align: left;
  font-family: PingFangSC-regular;
  width: 80vw;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}

.line {
  /* position: absolute; */
  width: 750rpx;
  border: 1rpx solid rgba(187, 187, 187, 1);
}
.my-choosebox checkbox[checked] {
  border: 4rpx solid transparent;
  background: white;
}
.next-size {
  z-index: 0;
}
  

