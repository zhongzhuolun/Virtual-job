
page {
  background-color: rgba(255, 255, 255, 1);
}
.bank-wrap {
  background: white;
  padding-bottom: 5vh;
}
.bank-title {
  margin-left: 34rpx;
  padding-top: 42rpx;
  color: rgba(16, 16, 16, 1);
  font-size: 50rpx;
  text-align: left;
  font-family: PingFangSC-regular;
  width: 80vw;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.line {
  width: 750rpx;
  border: 1rpx solid rgba(187, 187, 187, 1);
}
.bank {
  margin-left: 34rpx;
  padding-top: 45rpx;
  height: 57vh;
  overflow: auto;
}
.checkout-question {
  margin-top: 20rpx;
}
.audio-play {
  width: 120rpx;
  height: 60rpx;
  border: 2rpx solid #FABF01;
  border-radius: 20rpx 20rpx;
  font-size: 40rpx;
  text-align: center;
  line-height: 60rpx;
}
.audio-play .cuIcon-notificationfill  {
  margin-left: 2rpx;
}
.audio-play .cuIcon-playfill, .audio-play .cuIcon-stop{
  font-size: 40rpx;
  border-radius: 50% 50%;
  border: 1rpx solid;
  padding-left: 3rpx;
}
