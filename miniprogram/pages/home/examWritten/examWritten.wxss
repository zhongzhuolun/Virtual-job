scroll-view {
  background: white;
}
.bank-wrap {
  background: white;
  padding-bottom: 200rpx;
}
.bank-title {
  margin-left: 34rpx;
  padding-top: 42rpx;
  color: rgba(16, 16, 16, 1);
  font-size: 50rpx;
  text-align: left;
  font-family: PingFangSC-regular;
}
.score-time {
  margin-left: 34rpx;
  margin-top: 1vh;
  margin-bottom: 30rpx;
  color: rgba(16, 16, 16, 1);
  font-size: 28rpx;
  text-align: left;
  font-family: PingFangSC-regular;
}
.line {
  width: 750rpx;
  border: 1rpx solid rgba(187, 187, 187, 1);
}
.bank {
  margin-left: 34rpx;
  padding-top: 45rpx;
}
.my-choosebox radio {
  border: 4rpx solid #ffc200;
}
.my-choosebox radio[checked] {
  border: 4rpx solid black;
}
.my-choosebox checkbox[checked] {
  border: 4rpx solid black;
  background: black;
}

.submit {
  position: relative;
  left: 254rpx;
  width: 242rpx;
  height: 70rpx;
  line-height: 64rpx;
  border-radius: 20rpx;
  background-color: rgba(250, 191, 1, 1);
  color: rgba(16, 16, 16, 1);
  font-size: 36rpx;
  text-align: center;
  font-family: PingFang SC;
  border: 4rpx solid rgba(0, 0, 0, 1);
  transform: translate3d(0, 100%, 0);
}
.timer {
  position: fixed;
  left: 550rpx;
  bottom: 40rpx;
  width: 168rpx;
  height: 70rpx;
  line-height: 64rpx;
  border-radius: 52rpx;
  color: rgba(250, 191, 1, 1);
  font-size: 36rpx;
  text-align: center;
  box-shadow: 0rpx 4rpx 4rpx 0rpx rgba(0, 0, 0, 0.24);
  font-family: -apple-system;
  border: 4rpx solid rgba(250, 191, 1, 1);
}
.bg-sbumit {
  background: rgba(250, 191, 1, 1);
  position: absolute;
  height: 100vh;
  width: 100vw;
  z-index: 100;
  top: 0;
  left: 0;
}
.detail {
  position: absolute;
  left: 66rpx;
  top: 44%;
  transform: translateY(-50%);
  /* top: 314rpx; */
  width: 616rpx;
  height: 430rpx;
  line-height: 52rpx;
  border-radius: 50rpx;
  background-color: rgba(255, 255, 255, 0.47);
  color: rgba(16, 16, 16, 1);
  font-size: 36rpx;
  text-align: center;
  box-shadow: 0rpx 8rpx 18rpx 0rpx rgba(178, 178, 178, 0.4);
  font-family: -apple-system;
  border: 1rpx solid rgba(255, 255, 255, 0);
  padding: 120rpx;
  line-height: 64rpx;
}
.show {
  display: block;
  visibility: visible;
}
.hide {
  display: none;
  visibility: hidden;
  height: 100vh;
  overflow: hidden;
}

.my-btn {
  position: absolute;
  width: 246rpx;
  height: 94rpx;
  /* top: 796rpx; */
  bottom: 26%;

  line-height: 90rpx;
  border-radius: 20rpx;
  background-color: rgba(250, 191, 1, 1);
  color: rgba(16, 16, 16, 1);
  font-size: 36rpx;
  text-align: center;
  box-shadow: 0rpx 4rpx 12rpx 0rpx rgba(0, 0, 0, 0.33);
  font-family: PingFang SC;
  border: 4rpx solid rgba(0, 0, 0, 1);
}
.unfinished .continute {
  left: 112rpx;
}
.unfinished .complete {
  left: 408rpx;
}

