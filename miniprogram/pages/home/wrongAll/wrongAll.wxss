.radio-question,
.checkbox-question,
.blank-question,
.sort-question {
  display: flex;
  flex-direction: column;
  color: rgba(16, 16, 16, 1);
  font-size: 30rpx;
  text-align: left;
  font-family: PingFangSC-regular;
  margin-bottom: 15rpx;
}

.my-choosebox {
  display: block;
}
.my-choosebox text {
  margin-left: 20rpx;
}
.my-choosebox radio {
  transform: scale(0.6);
  border-radius: 50%;
  border: 4rpx solid #ffc200;
  background: transparent;
}
 .my-choosebox checkbox {
  transform: scale(0.6);
  border: 4rpx solid #ffc200;
  background: transparent;
}
.blank-question input, .sort-question input {
  width: 690rpx;
  height: 72rpx;
  color: rgba(136, 136, 136, 1);
  font-size: 28rpx;
  text-align: left;
  font-family: -apple-system;
  border: 1rpx solid rgba(187, 187, 187, 1);
  padding-left: 1em;
  margin-top: 10rpx;
}

.correct-analyse {
  margin-top: 30rpx;
  color: rgba(16, 16, 16, 1);
  font-size: 36rpx;
  text-align: left;
  font-family: PingFangSC-regular;
}
.comments {
  padding: 30rpx;
}
.comment {
  position: relative;
}
.comment .user {
  display: flex;
  justify-content: space-between;
}
.like-dot, .user-info {
  display: flex;
}
.user-info .user-name {
  margin-left: 10rpx;
}
.like-dot {
  margin-right: 20rpx;
}

.comment .mine {
  width: 40rpx;
  height: 40rpx;
}
.comment .like {
  width: 40rpx;
  height: 40rpx;
}
.comment .content {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  padding: 20rpx;
  text-overflow: ellipsis;
  height: 100rpx;
  overflow: hidden;
  color: rgba(16, 16, 16, 1);
  font-size: 32rpx;
  text-align: left;
  font-family: PingFangSC-regular;
}
.view-all {
  height: 40rpx;
  color: rgba(163, 163, 163, 1);
  font-size: 28rpx;
  text-align: center;
  font-family: PingFangSC-regular;
  text-decoration: underline;
  margin-top: 10rpx;
}
