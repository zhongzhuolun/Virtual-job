.replay .replay-info .replay-name {
  color: rgba(16, 16, 16, 1);
  font-family: SourceHanSansSC-regular;
  padding-left: 10rpx;
}
.replay .replay-info image {
  height: 40rpx;
  width: 40rpx;
  border-radius: 100%;
}

.view-all {
