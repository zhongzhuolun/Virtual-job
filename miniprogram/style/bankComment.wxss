.next-publish .pre-question {
  margin-left: 3.5vw;
}


.page-size {
  margin: 0 auto;
  width: 60rpx;
  height: 40rpx;
  color: rgba(16, 16, 16, 1);
  font-size: 28rpx;
  text-align: left;
  font-family: PingFangSC-regular;
  padding-top: 100rpx;
}

.type-btn  {
  font-size: 24rpx;
  width: 160rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 8rpx;
  background-color: rgba(250, 191, 1, 1);
  color: rgba(16, 16, 16, 1);
  text-align: center;
  font-family: PingFang SC;
  margin: 0 auto;
}
.cu-dialog {
  background: white;
  border-radius: 11px;
  box-shadow: 0rpx 2rpx 8rpx 0rpx rgba(250, 191, 1, 0.47);
  border: 4rpx solid rgba(250, 191, 1, 1);
}
.cu-dialog .cubar {
  padding-top: 30rpx;
}
.question-index-wrap {
  display: flex;
  padding: 80rpx;
  padding-top: 40rpx;
  flex-wrap: wrap;
  justify-content: space-around;
  background: white;
}
.cu-bar {
  min-height: 40rpx;
}
.bank-wrap .cu-modal .cu-dialog>.cu-bar:first-child .action {
  margin-top: 20rpx;
  min-width: 100rpx;
  margin-right: 0;
  min-height: 40rpx;
  
  }
  
.question-index-wrap .question-index {
  width: 72rpx;
  height: 72rpx;
  line-height: 72rpx;
  background-color: rgba(255, 255, 255, 0);
  font-size: 36rpx;
  text-align: center;
  font-family: -apple-system;
  border-radius: 50% 50%;
  margin: 10rpx;
  border: 2rpx solid black;
}
.question-index-wrap .correct {
  border: 2rpx solid rgba(250, 191, 1, 1);
  color: rgba(250, 191, 1, 1);
}

.question-index-wrap .wrong {
  border: 2rpx solid red;
  color: rgb(250, 1, 1);
}


.no-comments {
  text-align: center;
}
.view-answer {
  width: 112rpx;
  height: 40rpx;
  color: rgba(139, 137, 129, 1);
  font-size: 28rpx;
  text-align: left;
  font-family: PingFangSC-regular;
  border-bottom: 2rpx solid rgba(187, 187, 187, 1);
  margin-top: 18rpx;
}
.wrong-blank {
  color: red;
}
.correct-blank {
  color: #fabf01;
}
.radio-question,
.checkbox-question,
.blank-question,
.sort-question {
  display: flex;
  flex-direction: column;
  color: rgba(16, 16, 16, 1);
  font-size: 30rpx;
  text-align: left;
  font-family: PingFangSC-regular;
  margin-bottom: 15rpx;
  padding-right: 5vw;
}

.my-choosebox {
  display: block;
}
.my-choosebox text {
  margin-left: 20rpx;
}

.my-choosebox radio {
  transform: scale(0.6);
  border-radius: 50%;
  border: 4rpx solid transparent;
  background: transparent;
}
.my-choosebox .no-radio {
  border: 4rpx solid rgba(250, 191, 1, 1);
}
 .my-choosebox checkbox {
  transform: scale(0.6);
  border: 4rpx solid rgba(250, 191, 1, 1);
  background: transparent;
  border-radius: 10rpx 10rpx
}
.my-choosebox .no-checkbox {
  border: 4rpx solid rgba(250, 191, 1, 1);
}
checkbox .wx-checkbox-input {
  border: none;
}
radio .wx-radio-input {
  border: none;
}
radio .wx-radio-input.wx-radio-input-disabled {
  background: white;
}

checkbox .wx-checkbox-input.wx-checkbox-input-disabled {
  background: white;
}
radio.red {
  border: 4rpx solid red !important;
}
checkbox.red {
  border: 4rpx solid red !important;
}
.yourblank {
  font-size: 36rpx;
  margin-top: 28rpx;
  font-family: PingFangSC-regular;
}
.question-title text {
  width: 100%;
  word-break: break-all;
  /* display: inline-block; */
}
.blank-question input, .sort-question input {
  width: 680rpx;
  height: 72rpx;
  color: rgba(136, 136, 136, 1);
  font-size: 28rpx;
  text-align: left;
  font-family: -apple-system;
  border: 1rpx solid rgba(187, 187, 187, 1);
  padding-left: 1em;
  margin-top: 10rpx;
}