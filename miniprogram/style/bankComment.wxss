.next-publish {
  position: fixed;
  left: 0px;
  bottom: 0;
  display: flex;
  width: 100%;
  height: 122rpx;
  line-height: 40rpx;
  text-align: center;
  box-shadow: 0rpx -4rpx 14rpx -8rpx rgba(163, 163, 163, 0.83);
  border: 2rpx solid rgba(255, 255, 255, 0);
  background: white;
}
.next-publish .publish {
  display: flex;
  width: 70vw;
  border-bottom: 2rpx solid rgba(187, 187, 187, 1);
  height: 100rpx;
  padding: 26rpx;
  margin: 0 10rpx;
}
.next-publish .publish input {
  width: 75%;
  text-align: left;
  margin-top: 9rpx;
}
.next-publish .publish .publish-comment {
  width: 116rpx;
  height: 60rpx;
  border-radius: 8rpx;
  background-color: rgba(250, 191, 1, 1);
  color: rgba(16, 16, 16, 1);
  font-size: 28rpx;
  text-align: center;
  box-shadow: 2rpx 2rpx 12rpx 0rpx rgba(250, 191, 1, 0.7);
  font-family: PingFang SC;
  border: 2rpx solid rgba(255, 255, 255, 0);
  line-height: 56rpx;
}

.next-publish .chekckout-question {
  width: 10vw;
  line-height: 122rpx;
  font-size: 76rpx;
}
